{
  "name": "dog-and-pony",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "ci": "yarn lint && yarn type-check",
    "dev": "next dev",
    "lint": "eslint src --max-warnings=0",
    "lint:fix": "yarn lint --fix",
    "prepare": "husky install",
    "start": "next start",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "type-check:watch": "yarn type-check -w",
    "storybook": "storybook dev -p 6006 -s ../public",
    "build-storybook": "storybook build -s public",
    "generate": "pnpm plop --plopfile .generate/plopfile.js"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn lint --fix"
    ]
  },
  "dependencies": {
    "next": "13.4.1",
    "react": "^18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.7.0",
    "@hookform/resolvers": "^3.3.0",
    "@radix-ui/react-accessible-icon": "1.0.2",
    "@radix-ui/react-accordion": "1.1.1",
    "@radix-ui/react-collapsible": "1.0.2",
    "@radix-ui/react-label": "2.0.1",
    "@radix-ui/react-separator": "1.0.2",
    "@radix-ui/react-toast": "1.1.3",
    "clsx": "1.2.1",
    "react-hook-form": "7.43.9",
    "uuid": "9.0.0",
    "zod": "3.21.4",
    "zustand": "^4.3.2",
    "@storybook/addon-actions": "^7.0.7",
    "@storybook/addon-essentials": "^7.0.7",
    "@storybook/addon-interactions": "^7.0.7",
    "@storybook/addon-links": "^7.0.7",
    "@storybook/addon-styling": "^1.0.3",
    "@storybook/addons": "^7.0.7",
    "@storybook/blocks": "^7.0.7",
    "@storybook/client-api": "^7.0.7",
    "@storybook/manager-api": "^7.0.7",
    "@storybook/nextjs": "7.0.7",
    "@storybook/react": "^7.0.7",
    "@storybook/testing-library": "^0.1.0",
    "@storybook/theming": "^7.0.7",
    "eslint-plugin-storybook": "^0.6.11",
    "storybook": "^7.0.7",
    "@types/node": "20.1.3",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@types/uuid": "9.0.1",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "@typescript-eslint/parser": "5.59.5",
    "autoprefixer": "10.4.14",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.1",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import-helpers": "1.3.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-tailwindcss": "3.11.0",
    "husky": "8.0.3",
    "immer": "^10.0.2",
    "lint-staged": "13.2.2",
    "plop": "^3.1.2",
    "postcss": "8.4.23",
    "prettier": "2.8.8",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4"
  }
}
